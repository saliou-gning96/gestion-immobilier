{% extends 'base.html.twig' %}

{% block title %}
  TypeBien
{% endblock %}

{% block content %}
  <!-- Page-Title -->
  <div class="row">
    <div class="col-sm-12">
      <h4 class="pull-left page-title">
        Détail
      </h4>
      <ol class="breadcrumb pull-right">
        <li><a href="/">Accueil</a></li>
        <li><a href="{{ path('type_bien_index') }}">Types de biens</a></li>
        <li class="active">Détail</li>
      </ol>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-border panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">
            Détail du type de bien
          </h3>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-sm-6"></div>
                <div class="col-sm-6">
                <div class="m-b-30 pull-right">
                    <a href="{{ path('type_bien_index') }}"
                    class="btn btn-primary waves-effect waves-light">
                    Voir la liste
                    </a>
                </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <table class="table table-striped table-bordered">
                        <tbody>
                            <tr>
                                <th>
                                Id
                                </th>
                                <td>
                                {{ type_bien.id }}
                                </td>
                            </tr>
                            <tr>
                                <th>
                                Libelle
                                </th>
                                <td>
                                {{ type_bien.libelle }}
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    {{ include('type_bien/_delete_form.html.twig') }}
                </div>
            </div>
        </div>
    </div>

 
{% endblock %}
