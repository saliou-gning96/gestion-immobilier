<form method="post" action="{{ path('proprietaire_delete', {'id': proprietaire.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ proprietaire.id) }}">
    <button class="btn btn-danger waves-effect waves-light">Delete</button>
</form>
